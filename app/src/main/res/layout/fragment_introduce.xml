<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:binding="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".aioui.main.MainActivity">

    <data>

        <variable
            name="viewModel"
            type="com.goldze.mvvmhabit.aioui.main.fragment.IntroduceFgViewModel" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <com.goldze.mvvmhabit.ui.base.widget.BaseRootView
            android:id="@+id/baseRoot"
            android:layout_width="match_parent"
            android:layout_height="@dimen/titleBar_height" />

        <WebView
            android:id="@+id/webView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="60px"
            android:layout_marginTop="360px"
            android:layout_marginRight="60px" />


    </FrameLayout>
</layout>